{
  "name": "helseriet-backend",
  "version": "1.0.0",
  "description": "Helseriet E-commerce Backend API",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node -r tsconfig-paths/register prisma/seeds/index.ts",
    "upload-images": "ts-node --transpile-only -r tsconfig-paths/register scripts/bulk-upload-images.ts",
    "upload-organized": "ts-node --transpile-only -r tsconfig-paths/register scripts/organized-bulk-upload.ts",
    "cleanup-cloudinary": "ts-node --transpile-only -r tsconfig-paths/register scripts/cleanup-cloudinary.ts",
    "upload-direct": "ts-node --transpile-only -r tsconfig-paths/register scripts/direct-upload.ts",
    "check-cloudinary": "ts-node --transpile-only -r tsconfig-paths/register scripts/check-cloudinary.ts",
    "list-images": "ts-node --transpile-only -r tsconfig-paths/register scripts/list-images.ts",
    "lint": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecommerce",
    "api",
    "express",
    "typescript",
    "prisma"
  ],
  "author": "Helseriet",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "prisma": "^6.13.0",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.1",
    "@types/sharp": "^0.31.1",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
