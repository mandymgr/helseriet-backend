{
  "name": "helseriet-backend",
  "version": "1.0.0",
  "description": "Helseriet E-commerce Backend API",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node -r tsconfig-paths/register prisma/seeds/index.ts",
    "upload-images": "ts-node --transpile-only -r tsconfig-paths/register scripts/bulk-upload-images.ts",
    "upload-organized": "ts-node --transpile-only -r tsconfig-paths/register scripts/organized-bulk-upload.ts",
    "cleanup-cloudinary": "ts-node --transpile-only -r tsconfig-paths/register scripts/cleanup-cloudinary.ts",
    "upload-direct": "ts-node --transpile-only -r tsconfig-paths/register scripts/direct-upload.ts",
    "check-cloudinary": "ts-node --transpile-only -r tsconfig-paths/register scripts/check-cloudinary.ts",
    "list-images": "ts-node --transpile-only -r tsconfig-paths/register scripts/list-images.ts",
    "lint": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "ecommerce",
    "api",
    "express",
    "typescript",
    "prisma"
  ],
  "author": "Helseriet",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@sentry/node": "^10.5.0",
    "@sentry/tracing": "^7.120.4",
    "@types/axios": "^0.9.36",
    "@types/express-session": "^1.18.2",
    "@types/node-fetch": "^2.6.13",
    "@types/nodemailer": "^7.0.1",
    "@types/stripe": "^8.0.416",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "prisma": "^6.13.0",
    "sharp": "^0.34.3",
    "stripe": "^18.4.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@sentry/profiling-node": "^10.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.1",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "concurrently": "^9.2.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "source-map-explorer": "^2.5.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
